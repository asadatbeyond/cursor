{
  "language": {
    "feature": "en-US"
  },
  "bindingCulture": {
    "name": "en-US"
  },
  "framework": {
    "name": "NUnit"
  },
  "stepAssemblies": [
    {
      "assembly": "SampleApiTests"
    }
  ],
  "trace": {
    "traceSuccessfulSteps": true,
    "traceTimings": true,
    "minTracedDuration": "0:00:00.1"
  },
  "generator": {
    "allowDebugGeneratedFiles": false,
    "allowRowTests": true,
    "generateAsyncTests": true,
    "addNonParallelizableMarkerForTags": []
  }
}
